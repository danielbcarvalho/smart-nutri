{
  "name": "smartnutri-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:prod": "NODE_ENV=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "NODE_ENV=development nest start --watch",
    "dev:prod": "NODE_ENV=production nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "setup:env": "node scripts/setup-env.js",
    "setup:env:dev": "NODE_ENV=development node scripts/setup-env.js",
    "setup:env:prod": "NODE_ENV=production node scripts/setup-env.js",
    "setup:env:test": "NODE_ENV=test node scripts/setup-env.js",
    "test": "node scripts/clean-test-db.js && jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:api": "chmod +x ./test-api.sh && ./test-api.sh",
    "test:dredd": "chmod +x ./test-dredd.sh && ./test-dredd.sh",
    "generate:swagger": "node generate-swagger-json.js",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/config/typeorm.migrations.config.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/config/typeorm.migrations.config.ts",
    "migration:run:prod": "NODE_ENV=production node scripts/setup-env.js && typeorm-ts-node-commonjs migration:run -d src/config/typeorm.migrations.config.ts",
    "migration:run:dev": "NODE_ENV=development node scripts/setup-env.js && typeorm-ts-node-commonjs migration:run -d src/config/typeorm.migrations.config.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/config/typeorm.migrations.config.ts",
    "passwords:migrate": "ts-node src/encryption/migrate-passwords.ts",
    "password:decrypt": "ts-node src/encryption/decrypt-password.ts",
    "test:encrypt": "ts-node src/encryption/test-encrypt.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/typeorm": "^10.0.1",
    "@supabase/supabase-js": "^2.49.4",
    "@types/cheerio": "^0.22.35",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto-js": "^4.2.0",
    "fatsecret": "^1.0.2",
    "https-proxy-agent": "^7.0.6",
    "mongodb": "^6.16.0",
    "mongoose": "^8.14.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.3.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}