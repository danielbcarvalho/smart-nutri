{
  "name": "smartnutri-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:prod": "NODE_ENV=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "NODE_ENV=development nest start --watch",
    "dev:prod": "NODE_ENV=production nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "setup:env": "node scripts/setup-env.js",
    "setup:env:dev": "NODE_ENV=development node scripts/setup-env.js",
    "setup:env:prod": "NODE_ENV=production node scripts/setup-env.js",
    "test": "node scripts/clean-test-db.js && jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:api": "chmod +x ./test-api.sh && ./test-api.sh",
    "test:dredd": "chmod +x ./test-dredd.sh && ./test-dredd.sh",
    "generate:swagger": "node generate-swagger-json.js",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/config/typeorm.migrations.config.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/config/typeorm.migrations.config.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/config/typeorm.migrations.config.ts",
    "passwords:migrate": "ts-node src/encryption/migrate-passwords.ts",
    "password:decrypt": "ts-node src/encryption/decrypt-password.ts",
    "test:encrypt": "ts-node src/encryption/test-encrypt.ts",
    "test:password-api": "ts-node src/encryption/test-api.ts"
  }
}